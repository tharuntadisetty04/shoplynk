{
  "functions": {
    "api/pingServer.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "cron": {
    "jobs": [
      {
        "path": "/api/pingServer",
        "schedule": "*/5 * * * *"
      }
    ]
  }
}
