{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/pingServer.js": {
      "runtime": "nodejs18.x"
    }
  },
  "builds": [
    {
      "src": "api/pingServer.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/pingServer",
      "dest": "/api/pingServer.js"
    }
  ]
}
